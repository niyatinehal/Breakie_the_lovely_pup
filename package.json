{
  "name": "puppy-flirts",
  "displayName": "Puppy Flirts",
  "description": "A cute dog that flirts with you while coding 🐶💘",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "puppy-flirts.start",
        "title": "Start Puppy Compliments"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -p ./"
  },
  "devDependencies": {
    "@types/node": "^22.15.18",
    "@types/vscode": "^1.70.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.0"
  }
}
